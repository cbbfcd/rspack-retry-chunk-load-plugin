  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "peerDependencies": {
    "webpack": ">=5.0.0",
    "@rspack/core": "^0.7"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.12",
    "@types/prettier": "2.7.3",
    "@types/memory-fs": "^0.3.3",
    "@types/node": "^16.11.33",
    "@types/webpack": "^5.28.0",
    "codecov": "^3.8.1",
    "express": "^4.18.1",
    "gts": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.2.0",
    "jest": "^29.7.0",
    "memory-fs": "^0.5.0",
    "nodemon": "^2.0.16",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "5.5.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "@rspack/core": "0.7.4",
    "@rspack/cli": "0.7.4"
  },
  "scripts": {
    "lint": "gts lint",
    "test": "jest",
    "test:watch": "npm t -- --watch",
    "test:ci": "npm t -- --runInBand --coverage",
    "codecov": "cat coverage/lcov.info | codecov",
    "e2e:webpack": "webpack --config test/e2e/webpack.config.js && nodemon test/e2e/server.ts",
    "e2e:rspack": "rspack --config test/e2e/rspack.config.js && nodemon test/e2e/server.ts",
    "ci": "npm run lint && npm run test:ci",
    "prepare": "npm run compile",
    "clean": "gts clean",
    "compile": "tsc -p tsconfig.build.json",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/webpack-retry-chunk-load-plugin.git"
  },
  "keywords": [
    "webpack",
    "webpack plugin"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/webpack-retry-chunk-load-plugin/issues"
